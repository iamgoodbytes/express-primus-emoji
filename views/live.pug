extends layout

block content
  .app
    h1 Emoji, they're aliiiivee!
    div.emoji.emoji__live
      a(href="#" class="emo__shit" title="like shit" data-emo="shit" data-zoom="100") 💩
      a(href="#" class="emo__angry" title="I'm so angry!" data-emo="angry" data-zoom="100") 😡
      a(href="#" class="emo__neutral" title="meh" data-emo="neutral" data-zoom="100") 😐
      a(href="#" class="emo__happy" title="wo-hoow" data-emo="happy" data-zoom="100") 😃
      a(href="#" class="emo__unicorn" title="like a unicorn" data-emo="unicorn" data-zoom="100") 🦄 